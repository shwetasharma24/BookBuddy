    "@mui-treasury/styles": "^1.13.0",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "clsx": "^1.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "date-and-time": "^0.14.1",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "history": "^5.0.0",
    "is-equal": "^1.6.1",
    "jsonwebtoken": "^8.5.1",
    "material-ui-popup-state": "^1.6.1",
    "moment": "^2.24.0",
